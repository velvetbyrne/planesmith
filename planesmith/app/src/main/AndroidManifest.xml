<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="umn.ac.id.planesmith">

    <uses-permission
        android:name="android.permission.WRITE_EXTERNAL_STORAGE"
        android:maxSdkVersion="18" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Planesmith">
        <activity
            android:name=".Character_Add"
            android:exported="false" />
        <activity
            android:name=".World_Add"
            android:exported="false" />
        <activity
            android:name=".Story_Add"
            android:exported="false" />
        <activity
            android:name=".StoryAdd"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Characters" />
        <activity android:name=".Story" />
        <activity android:name=".World" />
        <activity android:name=".Register"/>
    </application>

</manifest>